{
  "author": "codeiqne",
  "bin": {
    "ts": "bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-autocomplete": "^3.0.2",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-not-found": "^3.0.4",
    "@oclif/plugin-plugins": "^4",
    "@oclif/plugin-warn-if-update-available": "^3.0.5",
    "conf-cli": "^0.1.9"
  },
  "description": "TSpace Development & Deployment Tool",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^3.17.2",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codeiqne/ts-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@tadamspace/cli",
  "oclif": {
    "bin": "ts",
    "dirname": "ts",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available",
      "conf-cli"
    ],
    "topicSeparator": ":",
    "topics": {
      "deploy": {
        "description": "deploy your app"
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://www.npmjs.com/package/@tadamspace/cli",
      "authorization": "Basic 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"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeiqne/ts-cli.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": {
    "url": "https://github.com/codeiqne/ts-cli/issues"
  },
  "keywords": [
    "oclif",
    "tspace"
  ],
  "types": "dist/index.d.ts"
}
